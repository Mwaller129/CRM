<%- include('../partials/header') %>
<h2>Client List</h2>
<table id="list">
    <thead>
        <tr>
            <th>Client Name</th>
        </tr>
    </thead>
    <tbody>
        <% accountHolders.forEach(function(a) { %>
            <tr>
                <td><%= a.clientName %></td>
                <td><a href="/accountHolders/<%= a._id %>">DETAILS</a></td>
            </tr>
        <% }) %>
    </tbody>
</table>
<h2>Activites</h2>
<table id="list">
    <thead>
        <tr>
            <th>Upcoming Activites</th> 
        </tr>
    </thead>
    <tbody>
        <% accountHolders.forEach(function(a) { %>
            <tr>
                <td><%= a.activies %></td>
                <td><a href="/accountHolders/<%= a._id %>">DETAILS</a></td>
            </tr>
        <% }) %>
    </tbody>
</table>
<%- include('../partials/footer') %>